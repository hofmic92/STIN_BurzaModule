{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StockApi": {
    "BaseUrl": "https://api.example.com/stocks",
    "HistoryDays": 10,
    "RetryCount": 5,
    "RetryDelaySeconds": 2
  },
  "NewsModule": {
    "RatingEndpoint": "http://localhost:8000/rating",
    "ListStockEndpoint": "http://localhost:8000/liststock",
    "SaleStockEndpoint": "http://localhost:8000/salestock"
  },
  "UserSettings": {
    "RatingThreshold": 5,
    "MinNewsCount": 3,
    "DefaultBuyAmount": 1,
    "ApplyFilter": true,
    "DeclineDays": 3,
    "MaxDeclines": 2
  },
  "Favorites": [
    "Microsoft",
    "Google",
    "Amazon",
    "Tesla",
    "Apple"
  ]
}